{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=8"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "db:init": "node scripts/mkdirs.mjs",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "sequelize-cli db:migrate",
    "db:migrate:undo": "sequelize-cli db:migrate:undo",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo:all",
    "db:sanity_check": "node scripts/run-psql.js ../scripts/sql/sanity_check.sql",
    "db:sanity_fix": "node scripts/run-psql.js ../scripts/sql/sanity_fix.sql",
    "test": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test mocha --timeout 15000 --reporter spec --file tests/setup/hooks.js \"tests/**/*.test.js\"",
    "test:watch": "pnpm test -- --watch",
    "test:cov": "cross-env NODE_ENV=test DOTENV_CONFIG_PATH=.env.test nyc mocha --timeout 15000 --reporter spec --file tests/setup/hooks.js \"tests/**/*.test.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3",
    "chai": "^4.5.0",
    "cross-env": "^7.0.3",
    "mocha": "^10.7.3",
    "nyc": "^15.1.0",
    "supertest": "^7.0.0"
  }
}